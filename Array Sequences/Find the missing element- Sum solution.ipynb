    "def finder(arr1, arr2):\n",
    "    s1=[]\n",
    "    s2=[]\n",
    "    sum1,sum2=0,0\n",
    "    for i in arr1:\n",
    "        sum1+=i\n",
    "        s1.append(sum1)\n",
    "    for j in arr2:\n",
    "        sum2+=j\n",
    "        s2.append(sum2)\n",
    "    return s1[-1]-s2[-1]\n"
   ]
